# IMPORTANT: Copy this file to .env.prod and fill with your actual production values
# NEVER commit .env.prod to git

# MySQL Database Configuration
MYSQL_ROOT_PASSWORD=your_very_strong_root_password_here
DB_USERNAME=togo_user
DB_PASSWORD=your_very_strong_db_password_here
DB_DATABASE=quincena_db

# Application Security
API_KEY=your_very_strong_api_key_minimum_32_characters
JWT_SECRET=your_jwt_secret_if_needed

# Email Configuration
RESEND_API_KEY=re_your_resend_api_key_here
RESEND_FROM_EMAIL=noreply@yourdomain.com

# Frontend Configuration
FRONTEND_ORIGIN=https://yourdomain.com
NEXT_PUBLIC_API_URL=https://yourdomain.com/api
NEXT_PUBLIC_API_KEY=your_very_strong_api_key_minimum_32_characters

# File Upload
MAX_FILE_SIZE=5242880
UPLOAD_PATH=/app/uploads

# Rate Limiting
RATE_LIMIT_TTL=60000
RATE_LIMIT_MAX=100

# Domain Configuration
DOMAIN=yourdomain.com
